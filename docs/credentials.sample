# CloudLens Credentials File
# Location: ~/.cloudlens/credentials
#
# This file follows the AWS CLI credentials format
# You can define multiple profiles

[default]
access_key_id = YOUR_ACCESS_KEY_ID
access_key_secret = YOUR_ACCESS_KEY_SECRET
provider = aliyun
region = cn-hangzhou

[production]
access_key_id = PROD_ACCESS_KEY_ID
access_key_secret = PROD_ACCESS_KEY_SECRET
provider = aliyun
region = cn-beijing

[tencent-dev]
access_key_id = TENCENT_KEY_ID
access_key_secret = TENCENT_KEY_SECRET
provider = tencent
region = ap-guangzhou
